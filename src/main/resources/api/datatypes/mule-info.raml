#%RAML 1.0 DataType
type: object
properties:
  appName: 
    description: Mulesoft application name
    type: string  
  runtimeVersion:
    type: string
    description: The mule runtime version the application is running on
    example: "4.2.0"
  environment:
    type: string
    description: Environment in which the application is running
    example: "DEV"
